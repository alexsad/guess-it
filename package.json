{
  "name": "find-me",
  "version": "0.0.0",
  "description": "a simple game made with Ferrugemjs",
  "main": false,
  "scripts": {
    "copy:interfaces": "copyfiles ../interfaces/*.ts src/interfaces",
    "copy:static": "copyfiles ./index.html ./offline.manifest ./app.manifest public",
    "watch": "webpack-dev-server --progress --colors --config webpack.config.js",
    "w:home": "webpack-dev-server --progress --colors --config webpack.config.js --host 192.168.1.106",
    "build": "npm run copy:static && npm run copy:interfaces && webpack -p --config webpack.config.js",
    "bundle:analyzer": "webpack --profile --json > stats.json && webpack-bundle-analyzer stats.json"
  },
  "author": "ferrugemjs",
  "license": "ISC",
  "dependencies": {
    "es6-shim": "^0.35.3",
    "ferrugemjs": "^1.5.2",
    "ferrugemjs-router": "^0.28.0",
    "js-cookie": "^2.2.0",
    "node-sass": "^4.9.0",
    "qrcode": "^1.2.0",
    "socket.io-client": "^2.1.0",
    "tslib": "^1.9.0",
    "v3rtigo": "^0.4.1",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "compression-webpack-plugin": "^1.1.11",
    "copyfiles": "^2.0.0",
    "css-loader": "^0.28.11",
    "ferrugemjs-loader": "^0.31.4",
    "file-loader": "^1.1.11",
    "http-proxy-middleware": "^0.18.0",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.3.0",
    "tslint": "^5.10.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^2.8.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.1",
    "webpack-bundle-analyzer": "^2.11.2",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  }
}

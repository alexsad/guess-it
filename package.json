{
  "name": "find-me",
  "version": "0.0.0",
  "description": "a simple game made with Ferrugemjs",
  "main": false,
  "scripts": {
    "copy:interfaces": "copyfiles ../interfaces/*.ts src/interfaces",
    "copy:index": "copyfiles ./index.html public",
    "watch": "webpack-dev-server --progress --colors --config webpack.config.js",
    "w:home": "webpack-dev-server --progress --colors --config webpack.config.js --host 192.168.0.100:3000",
    "build": "npm run copy:index && webpack --config webpack.config.js --optimize-minimize"
  },
  "author": "ferrugemjs",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "bootstrap-datepicker": "^1.6.4",
    "bootstrap-validator": "^0.11.9",
    "es6-shim": "^0.35.3",
    "event-emitter-lite": "^1.1.0",
    "ferrugemjs": "^0.17.3",
    "ferrugemjs-router": "^0.16.3",
    "jquery": "^3.2.1",
    "js-cookie": "^2.1.4",
    "moment": "^2.18.1",
    "selectize": "^0.12.4",
    "socket.io-client": "^2.0.3",
    "tslib": "^1.7.1",
    "twbs-pagination": "^1.4.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "copyfiles": "^1.2.0",
    "css-loader": "^0.28.4",
    "ferrugemjs-loader": "^0.16.3",
    "file-loader": "^0.11.2",
    "http-proxy-middleware": "^0.17.4",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.1.0",
    "typescript": "^2.3.3",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  }
}
